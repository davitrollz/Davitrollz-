local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("Davi Trollz Hub")
local Section = Window:NewSection("Main")

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local LocalPlayer = Players.LocalPlayer
local Z = ReplicatedStorage:WaitForChild("Z")
local ITS = ReplicatedStorage:WaitForChild("ITS")

Section:CreateButton("TP to End", function()
	LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(63.64146041870117, 479.9999694824219, 230.1397247314453)
end)

Section:CreateButton("Get SecretSlap", function()
	Z:FireServer("SecretSlap")
end)

Section:CreateButton("Get FireSlap", function()
	Z:FireServer("FireSlap")
end)

Section:CreateButton("Get SpeedCoil", function()
	Z:FireServer("SpeedCoil")
end)

Section:CreateButton("Get Rainbow Carpet", function()
	Z:FireServer("FreeRainbowCarpet")
end)

local slapAllEnabled = false

Section:CreateToggle("Auto Slap", function(state)
	slapAllEnabled = state
	if state then
		Z:FireServer("FreeSlap")
		task.wait(0.2)
		ITS:FireServer("Click", "FreeSlap")
		task.wait(0.2)

		task.spawn(function()
			while slapAllEnabled do
				for _, player in ipairs(Players:GetPlayers()) do
					if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
						local direction = Vector3.new(0, 0, 1)
						ITS:FireServer("slash", player.Character, direction)
					end
				end
				task.wait(1.11)
			end
		end)
	end
end)
